(display "testing string...")
(let ((str "我不明白你說什麼")
      (first (lambda (str) (string-ref str 0))))
  (assert (equal? (string-ref str 0) (first "我")))
  (assert (equal? (string-ref str 1) (first "不")))
  (assert (equal? (string-ref str 2) (first "明")))
  (assert (equal? (string-ref "kc" 1) (first "c")))
  (assert (equal? (string-length str) 8))
  (assert (equal? (string-append str str str) "我不明白你說什麼我不明白你說什麼我不明白你說什麼"))
  )
(print "ok")
