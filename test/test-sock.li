(let ((client-socket (make-client-socket "example.org" "80")))
  (socket-send client-socket "HEAD / HTTP/1.0\r\n")
  (socket-send client-socket "Host: example.org\r\n")
  (socket-send client-socket "\r\n")
  (let loop ((res (socket-recv client-socket 2)))
    (if (not (zero? (length res)))
      (begin (display res)
             (loop (socket-recv client-socket 2)))))
  # TODO: fix this: (socket-shutdown client-socket *shut-rd*)
  (socket-close client-socket))
